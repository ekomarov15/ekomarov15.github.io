datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

enum ExamType {
  OGE
  EGE
}

model Task {
  id     Int      @id @default(autoincrement())
  exam   ExamType
  number Int
  topic  String
  text   String
  answer String
}
